            "min",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "NOK/kWh",
          "decimals": 4,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "staircase": true,
            "showPoints": "never",
            "spanNulls": true,
            "gradientMode": "none"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Spot Price"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [
                    10,
                    5
                  ],
                  "fill": "dash"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Norgespris (active)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [
                    8,
                    8
                  ],
                  "fill": "dash"
                }
              },
              {
                "id": "custom.lineWidth",
                "value": 3
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total Cost"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.lineWidth",
                "value": 3
              },
              {
                "id": "custom.fillOpacity",
                "value": 20
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Cost Rate (kr/hour)",
      "type": "timeseries",
      "id": 13,
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 26
      },
