{
  "title": "Power Consumption",
  "uid": "power-consumption",
  "description": "Real-time power monitoring for CPU (RAPL) and GPU (hwmon). Works across all hosts with RAPL support.",
  "tags": [
    "provisioned",
    "shared",
    "power",
    "rapl",
    "monitoring"
  ],
  "editable": true,
  "schemaVersion": 41,
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "title": "Current CPU Power",
      "type": "stat",
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[$__rate_interval]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 65
              },
              {
                "color": "orange",
                "value": 105
              },
              {
                "color": "red",
                "value": 142
              }
            ]
          }
        }
      }
    },
    {
      "title": "Current GPU Power",
      "type": "stat",
      "id": 2,
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) > vector(0)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 150
              },
              {
                "color": "orange",
                "value": 200
              },
              {
                "color": "red",
                "value": 250
              }
            ]
          }
        }
      }
    },
    {
      "title": "Total System Power",
      "type": "stat",
      "id": 3,
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[$__rate_interval])) + (sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 200
              },
              {
                "color": "orange",
                "value": 300
              },
              {
                "color": "red",
                "value": 392
              }
            ]
          }
        }
      }
    },
    {
      "title": "Estimated Daily Cost",
      "type": "stat",
      "id": 4,
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[$__rate_interval])) + (sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0))) / 1000 * 24 * scalar(max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"}))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyNOK",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 25
              }
            ]
          }
        }
      }
    },
    {
      "title": "Peak CPU (24h)",
      "description": "Maximum CPU power in the last 24 hours. Shows N/A if less than 24h of data available.",
      "type": "stat",
      "id": 17,
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "max_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[24h:1m]) or max_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[1h:1m])",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "title": "Peak Total (24h)",
      "description": "Maximum total power in the last 24 hours. Shows N/A if less than 24h of data available.",
      "type": "stat",
      "id": 18,
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 21,
        "y": 0
      },
      "targets": [
        {
          "expr": "max_over_time((sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m])) + (sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0)))[24h:1m]) or max_over_time((sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m])) + (sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0)))[1h:1m])",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "title": "Current Total Cost",
      "type": "stat",
      "id": 11,
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 15,
        "y": 0
      },
      "targets": [
        {
          "expr": "max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"})",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "NOK/kWh",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1.0
              },
              {
                "color": "orange",
                "value": 1.5
              },
              {
                "color": "red",
                "value": 2.0
              }
            ]
          }
        }
      }
    },
    {
      "title": "Power Averages",
      "type": "table",
      "id": 5,
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "avg_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[1h:])",
          "legendFormat": "CPU",
          "refId": "A",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "avg_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[24h:])",
          "legendFormat": "CPU",
          "refId": "B",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "avg_over_time(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"})[1h:]) or vector(0)",
          "legendFormat": "GPU",
          "refId": "C",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "avg_over_time(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"})[24h:]) or vector(0)",
          "legendFormat": "GPU",
          "refId": "D",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "avg_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[1h:]) + (avg_over_time(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"})[1h:]) or vector(0))",
          "legendFormat": "Total",
          "refId": "E",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "avg_over_time(sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[1m]))[24h:]) + (avg_over_time(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"})[24h:]) or vector(0))",
          "legendFormat": "Total",
          "refId": "F",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "Value #A": "CPU 1h",
              "Value #B": "CPU 24h",
              "Value #C": "GPU 1h",
              "Value #D": "GPU 24h",
              "Value #E": "Total 1h",
              "Value #F": "Total 24h"
            },
            "indexByName": {
              "Value #A": 0,
              "Value #B": 1,
              "Value #C": 2,
              "Value #D": 3,
              "Value #E": 4,
              "Value #F": 5
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "md",
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "decimals": 1,
          "custom": {
            "align": "center",
            "cellOptions": {
              "type": "color-text"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "CPU.*"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "GPU.*"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "color",
                "value": {
                  "fixedColor": "purple",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Total.*"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "color",
                "value": {
                  "fixedColor": "semi-dark-yellow",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Cost (Last Hour)",
      "type": "stat",
      "id": 14,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "(sum(increase(node_rapl_package_joules_total{instance=~\"$instance\"}[1h])) / 3600000 + (avg_over_time(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"})[1h:]) or vector(0)) * 3600 / 3600000) * scalar(max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"}))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyNOK",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 2
              }
            ]
          }
        }
      }
    },
    {
      "title": "Power Consumption Over Time",
      "description": "Note: Legend stats (mean/max) use adaptive resolution. For longer time ranges, data is downsampled which may hide brief spikes. Use shorter ranges for accurate peak detection.",
      "type": "timeseries",
      "id": 8,
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "CPU",
          "refId": "A"
        },
        {
          "expr": "sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0)",
          "legendFormat": "GPU",
          "refId": "B"
        },
        {
          "expr": "sum(irate(node_rapl_package_joules_total{instance=~\"$instance\"}[$__rate_interval])) + (sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0))",
          "legendFormat": "Total",
          "refId": "C"
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "opacity",
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "CPU"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "GPU"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "purple",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [
                    10,
                    10
                  ],
                  "fill": "dash"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "CPU Core Power",
      "type": "timeseries",
      "id": 9,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "sum by (index) (irate(node_rapl_core_joules_total{instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "Core {{index}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "custom": {
            "lineWidth": 1,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "title": "Energy Consumed (Cumulative)",
      "type": "timeseries",
      "id": 10,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      },
      "targets": [
        {
          "expr": "sum(increase(node_rapl_package_joules_total{instance=~\"$instance\"}[$__range])) / 3600000",
          "legendFormat": "CPU (kWh)",
          "refId": "A"
        },
        {
          "expr": "(sum(node_hwmon_power_average_watt{instance=~\"$instance\", sensor=\"power1\"}) or vector(0)) * $__range_s / 3600000",
          "legendFormat": "GPU (kWh est.)",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "kwatth",
          "decimals": 1,
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "fillOpacity": 70,
            "lineWidth": 0,
            "showPoints": "never"
          },
          "color": {
            "mode": "palette-classic-by-name"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "CPU (kWh)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "#5794F2",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.drawStyle",
                "value": "line"
              },
              {
                "id": "custom.lineInterpolation",
                "value": "stepAfter"
              },
              {
                "id": "custom.lineWidth",
                "value": 2
              },
              {
                "id": "custom.fillOpacity",
                "value": 0
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "GPU (kWh est.)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "#FF9830",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.drawStyle",
                "value": "line"
              },
              {
                "id": "custom.lineWidth",
                "value": 2
              },
              {
                "id": "custom.fillOpacity",
                "value": 0
              },
              {
                "id": "custom.lineInterpolation",
                "value": "smooth"
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Electricity Price History",
      "type": "timeseries",
      "id": 12,
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 26
      },
      "targets": [
        {
          "expr": "electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"}",
          "legendFormat": "Spot Price",
          "refId": "A"
        },
        {
          "expr": "electricity_price_nok_per_kwh{tax=\"incl_mva\",type=\"norgespris\"}",
          "legendFormat": "Norgespris (active)",
          "refId": "B"
        },
        {
          "expr": "electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"}",
          "legendFormat": "Total Cost",
          "refId": "C"
        }
      ],
      "options": {
