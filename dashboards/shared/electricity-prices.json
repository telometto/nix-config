    {
      "title": "Norgespris Savings",
      "description": "Compares electricity costs with and without Norgespris. Shows spot price, total cost without Norgespris, total cost with Norgespris, and the savings. All prices in NOK/kWh.",
      "type": "timeseries",
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 17
      },
      "targets": [
        {
          "expr": "max(electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Spot Price",
          "refId": "A"
        },
        {
          "expr": "max(electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"}) + max(grid_tariff_energiledd_nok_per_kwh{tax=\"incl_mva\"}) + max(enova_avgift_nok_per_kwh{tax=\"incl_mva\"}) + max(elavgift_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Total Cost (without Norgespris)",
          "refId": "B"
        },
        {
          "expr": "max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Total Cost (with Norgespris)",
          "refId": "C"
        },
        {
          "expr": "(max(electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"}) + max(grid_tariff_energiledd_nok_per_kwh{tax=\"incl_mva\"}) + max(enova_avgift_nok_per_kwh{tax=\"incl_mva\"}) + max(elavgift_nok_per_kwh{tax=\"incl_mva\"})) - max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Savings (difference)",
          "refId": "D"
