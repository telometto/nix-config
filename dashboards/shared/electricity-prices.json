      },
      "fieldConfig": {
        "defaults": {
          "unit": "NOK/kWh",
          "decimals": 4,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": true
