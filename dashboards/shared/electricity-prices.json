{
  "title": "Electricity Prices",
  "uid": "electricity-prices",
  "description": "Comprehensive electricity price tracking including spot prices, Norgespris, grid tariff, and government taxes. Pure price monitoring without power consumption data.",
  "tags": [
    "provisioned",
    "shared",
    "electricity",
    "prices",
    "norgespris"
  ],
  "editable": true,
  "schemaVersion": 41,
  "refresh": "5m",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "title": "Current Total Cost",
      "description": "Total electricity cost per kWh including spot price (or Norgespris), grid tariff, and all government taxes. All prices are in NOK/kWh and including MVA.",
      "type": "stat",
      "id": 1,
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"}",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1.0
              },
              {
                "color": "orange",
                "value": 1.5
              },
              {
                "color": "red",
                "value": 2.0
              }
            ]
          }
        }
      }
    },
    {
      "title": "Current Spot Price",
      "description": "Current spot electricity price from hvakosterstrommen.no, including MVA. This is what you would pay without Norgespris. All prices are in NOK/kWh and including MVA.",
