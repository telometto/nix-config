    {
      "title": "Price Components Comparison",
      "type": "timeseries",
      "id": 6,
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "expr": "electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"}",
          "legendFormat": "Spot Price (what you'd pay without Norgespris)",
          "refId": "A"
        },
        {
          "expr": "electricity_price_nok_per_kwh{tax=\"incl_mva\",type=\"norgespris\"} and (time() >= 1736121600)",
          "legendFormat": "Norgespris (fixed 50 øre)",
          "refId": "B"
