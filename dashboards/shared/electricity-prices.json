    {
      "title": "Price Components Comparison",
      "description": "Comparison of all electricity price components over time: spot price, Norgespris fixed rate, grid tariff, and total cost. All prices are in NOK/kWh and including MVA.",
      "type": "timeseries",
      "id": 6,
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "expr": "electricity_spot_price_nok_per_kwh{tax=\"incl_mva\"}",
          "legendFormat": "Spot Price (without Norgespris)",
          "refId": "A"
        },
        {
          "expr": "electricity_price_nok_per_kwh{tax=\"incl_mva\",type=\"norgespris\"}",
          "legendFormat": "Norgespris (fixed to 50 øre, incl. MVA)",
          "refId": "B"
        },
        {
          "expr": "max(grid_tariff_energiledd_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Grid Tariff (Fagne AS)",
          "refId": "C"
        },
        {
          "expr": "max(electricity_total_cost_nok_per_kwh{tax=\"incl_mva\"})",
          "legendFormat": "Total Cost",
          "refId": "D"
        }
      ],
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "min",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 4,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": [
