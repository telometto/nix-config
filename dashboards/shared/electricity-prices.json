{
  "title": "Electricity Prices",
  "uid": "electricity-prices",
  "description": "Comprehensive electricity price tracking including spot prices, Norgespris, grid tariff, and government taxes. Pure price monitoring without power consumption data.",
  "tags": [
    "provisioned",
    "shared",
    "electricity",
    "prices",
    "norgespris"
  ],
  "editable": true,
  "schemaVersion": 41,
  "refresh": "5m",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
